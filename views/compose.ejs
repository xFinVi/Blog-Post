<%- include('partials/header') -%>
<h1>Compose</h1>

<form  action="/compose" method="post">
    <div class="form-group">
        <label for="postTitle">Title</label>
        <input class="form-control" type="text" name="postTitle">
    </div>
    <div class="form-group">
        <label for="postValue">Post</label>
        <textarea class="form-control" name="postValue" id="" cols="30" rows="10"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="submitBtn">Publish </button>
</form>

<%- include('partials/footer') -%>